{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/queries/*": ["queries/*"],
      "@/components/*": ["components/*"],
      "@/interfaces/*": ["interfaces/*"],
      "@/Storybook/*": ["Storybook/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "exclude": ["node_modules", ".next", "out", "_next"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js", "next.config.js"]
}
